{
  "name": "convert-apps-to-homebrew",
  "version": "1.0.0",
  "description": "Convert macOS applications to Homebrew installations with interactive selection",
  "keywords": [
    "homebrew",
    "macos",
    "applications",
    "cli",
    "package-manager",
    "automation",
    "typescript",
    "interactive",
    "cask",
    "formula",
    "installer",
    "migration",
    "apps",
    "brew"
  ],
  "homepage": "https://github.com/deepfriedmind/convert-apps-to-homebrew#readme",
  "bugs": {
    "url": "https://github.com/deepfriedmind/convert-apps-to-homebrew/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/deepfriedmind/convert-apps-to-homebrew.git"
  },
  "license": "MIT",
  "author": "deepfriedmind",
  "type": "module",
  "exports": "./dist/index.js",
  "main": "dist/index.js",
  "bin": "dist/index.js",
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "dist/**/*"
  ],
  "scripts": {
    "build": "npm run clean && tsc --project tsconfig.build.json",
    "check": "npm run lint:fix && npm run type-check && npm run knip && npm run test && npm run build",
    "clean": "del-cli dist/**/*",
    "knip": "knip",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "lint:inspect": "npx @eslint/config-inspector@latest",
    "start": "node src/index.ts",
    "test": "node --no-warnings --test --test-timeout=10000 'test/*.test.ts'",
    "test:coverage": "node --no-warnings --test --test-timeout=10000 --experimental-test-coverage 'test/*.test.ts'",
    "test:integration": "node --no-warnings --test --test-timeout=30000 'test/integration.test.ts'",
    "test:watch": "node --no-warnings --test --test-timeout=10000 --watch 'test/*.test.ts'",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@inquirer/checkbox": "^4.1.8",
    "bundle-name": "^4.1.0",
    "commander": "^14.0.0",
    "consola": "^3.4.2",
    "ora": "^8.2.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.14.1",
    "@tsconfig/node-ts": "^23.6.1",
    "@tsconfig/node24": "^24.0.1",
    "@types/node": "^24.0.0",
    "del-cli": "^6.0.0",
    "eslint": "^9.28.0",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-perfectionist": "^4.14.0",
    "knip": "^5.60.2",
    "typescript": "^5.8.3"
  },
  "packageManager": "npm@11.4.2",
  "engines": {
    "node": ">= 24",
    "npm": ">= 11"
  },
  "os": [
    "darwin"
  ]
}
